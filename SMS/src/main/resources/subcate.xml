<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="smssubcatens">
	<resultMap id="smssubcateResult" 	type="smssubcate">
		<result property="scate_no" 		column="scate_no" />
		<result property="mcate_no" 		column="mcate_no" />
		<result property="scate_name" 	column="scate_name" />
	</resultMap>
	<select id="list" resultMap="smssubcateResult">
		select * from sms_subcate
	</select>
	
	<select id="selectlist" parameterType="int" resultMap="smssubcateResult">
		select * from sms_subcate where mcate_no=#{mcate_no}
	</select>
	
</mapper>


